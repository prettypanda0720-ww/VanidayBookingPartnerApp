    "moment": "^2.24.0",
    "native-base": "^2.13.12",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-native": "^0.62.2",
    "react-native-actionsheet": "^2.4.2",
    "react-native-animatable": "^1.3.3",
    "react-native-calendar": "^0.13.1",
    "react-native-calendars": "^1.265.0",
    "react-native-checkbox": "^2.0.0",
    "react-native-collapsible": "^1.5.2",
    "react-native-country-modal": "^0.1.0",
    "react-native-country-picker-modal": "^1.10.0",
    "react-native-datepicker": "^1.7.2",
    "react-native-draggable-dynamic-flatlist": "^1.0.4",
    "react-native-drawer": "^2.5.1",
    "react-native-elements": "^2.0.2",
    "react-native-fast-image": "^8.1.5",
    "react-native-flash-message": "^0.1.15",
    "react-native-gesture-handler": "^1.6.1",
    "react-native-image-base64": "^0.1.4",
